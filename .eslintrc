{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],

  "rules": {
    "comma-dangle": "off",
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "quotes": ["error", "single"],
    "react/jsx-uses-vars": 1,
    "no-console": 1,
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  }
}
