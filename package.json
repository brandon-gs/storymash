  "name": "storymash",
  "version": "1.0.0",
  "description": "A social media of stories",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon backend/",
    "build": "next build frontend/ && tsc --project frontend/tsconfig.json",
    "start": "NODE_ENV=production node backend/",
    "type-check:front": "tsc --pretty --noEmit --project ./frontend/tsconfig.json",
    "format:front": "prettier --write ./frontend/**/*.ts{,x}",
    "lint:front": "yarn type-check:front && eslint -c .eslintrc.json ./**/*.ts{,x}",
    "test:front": "jest frontend/",
    "test-all:front": "yarn lint:front && yarn type-check:front && yarn test:front"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandon-gs/storymash.git"
  },
  "keywords": [
